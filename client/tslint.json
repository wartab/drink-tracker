{
    "defaultSeverity": "error",
    "jsRules": {},
    "rules": {
        "deprecation": {
            "severity": "warning"
        },
        "whitespace": {
            "options": [
                "check-branch",
                "check-decl",
                "check-operator",
                "check-separator",
                "check-type-operator",
                "check-preblock"
            ]
        },
        "class-name": true,
        "curly": true,
        "forin": true,
        "new-parens": true,
        "semicolon": true,
        "no-boolean-literal-compare": true,
        "use-isnan": true,
        "triple-equals": true,
        "function-constructor": true,
        "member-access": [
            true,
            "check-accessor",
            "check-constructor"
        ],
        "trailing-comma": {
            "options": {
                "singleline": "never",
                "esSpecCompliant": false,
                "multiline": {
                    "arrays": "always",
                    "exports": "never",
                    "functions": "ignore",
                    "imports": "never",
                    "objects": "always",
                    "typeLiterals": "ignore"
                }
            }
        },
        "quotemark": {
            "options": [
                true,
                "double",
                "avoid-escape"
            ]
        },
        "indent": [
            true,
            "spaces",
            4
        ],
        "no-for-in-array": true,
        "space-within-parens": {
            "options": [
                0
            ]
        },
        "no-var-keyword": true,
        "no-unused-expression": true,
        "no-duplicate-super": true,
        "no-duplicate-switch-case": true,
        "no-promise-as-boolean": true,
        "no-tautology-expression": true,
        "no-unsafe-finally": true,
        "radix": true,
        "restrict-plus-operands": true,
        "static-this": true,
        "strict-string-expressions": true,
        "unnecessary-constructor": true,
        "array-type": [
            true,
            "array"
        ]
    },
    "rulesDirectory": []
}
